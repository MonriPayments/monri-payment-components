{
  "name": "monri-payment-components",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prettier": "prettier --write \"**/*.{js,json,css,scss,less,md,ts,html,component.html}\"",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "prettify-keks-pay": "cd projects/libs/keks-pay && prettier --write \"**/*.{js,json,css,scss,less,md,ts,html,component.html}\" && cd ../../apps/keks-pay-app && prettier --write \"**/*.{js,json,css,scss,less,md,ts,html,component.html}\"",
    "prettify-apple-pay": "cd projects/libs/apple-pay && prettier --write \"**/*.{js,json,css,scss,less,md,ts,html,component.html}\" && cd ../../apps/apple-pay-app && prettier --write \"**/*.{js,json,css,scss,less,md,ts,html,component.html}\"",
    "build-keks-pay-library": "ng build keks-pay && ng build keks-pay-app && cd projects/apps/keks-pay-app && webpack && node build-bundles.js",
    "build-google-pay-library": "ng build google-pay && ng build google-pay-app && cd projects/apps/google-pay-app && webpack",
    "build-apple-pay-library": "ng build apple-pay && ng build apple-pay-app && cd projects/apps/apple-pay-app && webpack",
    "build-uac-library": "ng build uac && ng build uac-app && cd projects/apps/uac-app && webpack"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/elements": "^17.3.1",
    "@angular/forms": "^17.3.0",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "@ngrx/operators": "^17.1.1",
    "@ngrx/signals": "^17.1.1",
    "angularx-qrcode": "^17.0.0",
    "crypto-js": "^4.2.0",
    "rxjs": "~7.8.0",
    "shared": "^0.2.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.2",
    "@angular-eslint/builder": "17.3.0",
    "@angular-eslint/eslint-plugin": "17.3.0",
    "@angular-eslint/eslint-plugin-template": "17.3.0",
    "@angular-eslint/schematics": "17.3.0",
    "@angular-eslint/template-parser": "17.3.0",
    "@angular/cli": "^17.3.0",
    "@angular/compiler-cli": "^17.3.0",
    "@schuchard/prettier": "^5.1.0",
    "@types/crypto-js": "^4.2.2",
    "@types/jasmine": "~5.1.0",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "css-loader": "^6.10.0",
    "eslint": "^8.57.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^17.3.0",
    "prettier": "latest",
    "style-loader": "^3.3.4",
    "tslint-config-prettier": "latest",
    "typescript": "~5.4.2",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  }
}
